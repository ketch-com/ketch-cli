{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Regulation",
  "required": [
    "kind",
    "code",
    "name",
    "rightsFulfillment"
  ],
  "additionalProperties": false,
  "properties": {
    "kind": {
      "type": "string",
      "title": "Kind of custom object",
      "pattern": "regulation"
    },
    "code": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "rightsFulfillment": {
      "type": "object",
      "properties": {
        "rightCode": {
          "type": "string"
        },
        "fulfillment": {
          "type": "string"
        }
      }
    }
  }
}
