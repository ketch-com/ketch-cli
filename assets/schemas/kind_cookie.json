{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Cookie",
  "required": [
    "kind",
    "code",
    "name",
    "description"
  ],
  "additionalProperties": false,
  "properties": {
    "kind": {
      "type": "string",
      "title": "Kind of custom object",
      "pattern": "cookie"
    },
    "code": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "host": {
      "type": "string"
    },
    "duration": {
      "type": "string",
      "enum": [
        "session",
        "persistent"
      ]
    },
    "provenance": {
      "type": "string",
      "enum": [
        "firstParty",
        "thirdParty"
      ]
    },
    "category": {
      "type": "string",
      "enum": [
        "strictlyNecessary",
        "functional",
        "performance",
        "marketing",
        "analytics"
      ]
    },
    "serviceProvider": {
      "type": "string"
    },
    "translations": {
      "type": "object",
      "items": {
        "type": "object",
        "additionalProperties": true
      }
    }
  }
}
