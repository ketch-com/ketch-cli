  "required": [
    "kind",
    "code",
    "regulation",
    "legalBases"
  ],
  "additionalProperties": false,
  "properties": {
    "kind": {
      "type": "string",
      "title": "Kind of custom object",
      "pattern": "legalBasisRestriction"
    },
    "code": {
      "type": "string"
    },
    "regulation": {
      "type": "string"
    },
    "legalBases": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
